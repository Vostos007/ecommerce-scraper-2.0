{
  "name": "ui-dashboard",
  "private": true,
  "version": "0.1.3",
  "description": "Next.js 15.5.4 dashboard для управления Python экспортерами",
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.12.0",
  "scripts": {
    "dev": "next dev",
    "dashboard:dev": "node ./scripts/dashboard-dev.mjs",
    "build": "next build",
    "start": "next start",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .ts,.tsx",
    "lint:fix": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test --project=chromium",
    "auth:setup": "node ./scripts/auth-setup.mjs",
    "security:check": "pnpm lint && pnpm type-check && pnpm test",
    "audit:logs": "node ./scripts/audit-logs.mjs"
  },
  "dependencies": {
    "@hookform/resolvers": "3.10.0",
    "@tailwindcss/postcss": "4.0.0-beta.7",
    "@tanstack/react-query": "5.87.1",
    "@tanstack/react-query-devtools": "5.87.1",
    "archiver": "^7.0.1",
    "bcryptjs": "2.4.3",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cookie": "1.0.1",
    "helmet": "8.0.0",
    "jose": "5.9.6",
    "lucide-react": "0.468.0",
    "next": "15.5.4",
    "prom-client": "15.1.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-hook-form": "7.53.2",
    "redis": "4.7.0",
    "tailwind-merge": "2.5.4",
    "tailwindcss": "4.0.0-beta.7",
    "zod": "3.24.1",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.1",
    "@tailwindcss/typography": "0.5.15",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.1.0",
    "@testing-library/user-event": "14.5.2",
    "@types/archiver": "^6.0.3",
    "@types/bcryptjs": "2.4.6",
    "@types/cookie": "0.6.0",
    "@types/node": "22.12.0",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "@typescript-eslint/eslint-plugin": "8.44.1",
    "@typescript-eslint/parser": "8.44.1",
    "@vitejs/plugin-react": "4.3.4",
    "autoprefixer": "10.4.20",
    "eslint": "9.17.0",
    "eslint-config-next": "15.5.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react-hooks": "5.2.0",
    "jsdom": "25.0.1",
    "postcss": "8.5.1",
    "prettier": "3.4.2",
    "tailwindcss-animate": "1.0.7",
    "typescript": "5.9.2",
    "vite": "6.0.11",
    "vitest": "3.2.4"
  }
}
